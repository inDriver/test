{
  "name": "frontend-apollo-react-boilerplate",
  "version": "0.0.1",
  "description": "Starter kit for react app with apollo client gql",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack:dev",
    "webpack:build": "cross-env NODE_ENV=production webpack --mode=production --config configs/webpack/webpack.config.js --progress --colors",
    "webpack:dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config configs/webpack/webpack.config.js --progress --colors --host localhost --port 8080",
    "build": "rimraf ./build && npm run webpack:build",
    "build:analyze": "ANALYZE=true npm run build",
    "serve": "npx http-server build",
    "test": "jest --passWithNoTests --maxWorkers=4",
    "test:coverage": "jest --coverage --findRelatedTests --bail --maxWorkers=4",
    "lint": "eslint src/**/*.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lnt --quite",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inDriver/frontend-apollo-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "apollo",
    "gql"
  ],
  "author": "AndrewVorobev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inDriver/frontend-apollo-react-boilerplate/issues"
  },
  "homepage": "https://github.com/inDriver/frontend-apollo-react-boilerplate#readme",
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@indriver/ysera": "^0.19.0",
    "graphql": "^15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@hot-loader/react-dom": "^17.0.0",
    "@indriver/commitlint-config": "^1.0.0",
    "@indriver/eslint-config": "^1.0.7",
    "@indriver/prettier-config": "^1.0.0",
    "@indriver/stylelint-config": "^1.0.3",
    "@indriver/ts-config": "^1.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "commitlint": "^12.0.1",
    "compression-webpack-plugin": "^6.1.1",
    "copy-webpack-plugin": "^6.3.1",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gifsicle": "^5.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.1.3",
    "image-webpack-loader": "^7.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.0.0",
    "webpack-stats-plugin": "^1.0.2",
    "webpackbar": "^5.0.0-3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "tsc-files --noEmit",
      "npm run test",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
